<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
    <bean id="helloWorld" class="example.HelloWorld" init-method="init">
    	<constructor-arg value="10" />
    	<constructor-arg value="Hello" />
    	<property name="message" value="Hello World!" />
    </bean>
   <bean id="stu" class="example.Students" init-method="init">
   		<property name="htno" value="15481A0505" />
   		<property name="name" value="Aparna Akula" />
   		<property name="branch" value="CSE" />
   		<property name="clg" value="GEC" />
   </bean>  
   
   <bean id="human" class="example.Human" init-method="startMethod">
   		<property name="name" value="Aparna Akula" />
   		<property name="age" value="15" />
   </bean>  
   <bean class="examples.InitBeans" />
</beans>